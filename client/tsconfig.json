{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "ES6",
    "module": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "jsx": "react-native",
    "noEmit": true,
    "isolatedModules": true,
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  }
}
